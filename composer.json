{
  "name": "heller/laravel-updater",
  "description": "Tries to update laravel applications",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Lukas Heller",
      "email": "dev@lukas-heller.de"
    }
  ],
  "require-dev": {
    "laravel/pint": "dev-main",
    "pestphp/pest": "3.x-dev"
  },
  "bin": [
    "bin/updater"
  ],
  "autoload": {
    "psr-4": {
      "Heller\\LaravelUpdater\\": "src/"
    },
    "files": [
      "src/support/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
